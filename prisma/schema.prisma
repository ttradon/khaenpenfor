generator client {
  provider = "prisma-client-js"
  output   = "app/generated/prisma/client"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Couples {
  id      Int      @id @default(autoincrement())
  person1 String
  person2 String
  time    DateTime @default(now())
}
